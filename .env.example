# Toyota Corolla Trading Bot Environment Variables
# Copy this file to .env and fill in your values

# IBKR Connection
IBKR_HOST=127.0.0.1
IBKR_PORT=7497  # TWS Paper Trading port (7496 for live)
IBKR_CLIENT_ID=1

# Email Alerts (optional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_app_password
ALERT_TO=your_email@gmail.com

# Logging
LOG_LEVEL=INFO